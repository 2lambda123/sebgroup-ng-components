{
    "name": "@sebgroup/ng-components",
    "version": "0.0.0-semantically-released",
    "description": "A set of Angular components based on SEB design",
    "license": "Apache-2.0",
    "keywords": [
        "ng",
        "angular",
        "components",
        "javascript",
        "typescript",
        "frontend",
        "ui"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/sebgroup/ng-components.git"
    },
    "bugs": {
        "url": "https://github.com/sebgroup/ng-components/issues"
    },
    "homepage": "https://sebgroup.github.io/ng-components/",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve docs",
        "commit": "git-cz",
        "build": "ng build ng-components --prod",
        "test": "ng test ng-components",
        "lint": "ng lint ng-components",
        "e2e": "ng e2e ng-components",
        "format:fix": "pretty-quick --staged",
        "format:check": "pretty-quick --check --verbose",
        "format:all": "prettier --write \"**/*.{ts,html,json,scss,md}\""
    },
    "dependencies": {
        "@angular/animations": "~10.0.2",
        "@angular/cdk": "^10.0.1",
        "@angular/common": "~10.0.2",
        "@angular/compiler": "~10.0.2",
        "@angular/core": "~10.0.2",
        "@angular/forms": "~10.0.2",
        "@angular/platform-browser": "~10.0.2",
        "@angular/platform-browser-dynamic": "~10.0.2",
        "@angular/router": "~10.0.2",
        "@sebgroup/bootstrap": "^5.3.0",
        "@sebgroup/frontend-tools": "^1.6.0",
        "bootstrap-icons": "^1.0.0-alpha5",
        "rxjs": "~6.5.5",
        "tslib": "^2.0.0",
        "web-animations-js": "^2.3.2",
        "typescript-parser-es5": "^1.0.0",
        "zone.js": "~0.10.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1000.1",
        "@angular-devkit/build-ng-packagr": "~0.1000.1",
        "@angular/cli": "~10.0.1",
        "@angular/compiler-cli": "~10.0.2",
        "@commitlint/cli": "^9.0.1",
        "@commitlint/config-conventional": "^9.0.1",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "classlist.js": "^1.1.20150312",
        "codelyzer": "^6.0.0-next.1",
        "commitizen": "^4.1.2",
        "cz-conventional": "0.0.2",
        "jasmine-core": "~3.5.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~3.3.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "ng-packagr": "^10.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "protractor": "~7.0.0",
        "raw-loader": "^4.0.1",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~3.9.5"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint --edit",
            "pre-commit": "run-s format:fix package-lock-sanitizer"
        }
    },
    "browser": {
        "fs": false,
        "path": false,
        "os": false
    }
}
